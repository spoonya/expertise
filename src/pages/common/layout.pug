doctype html
html(lang="ru")
  block head
    head
      meta(charset="utf-8")
      meta(name="description" content="Экспертиза проектной документации в Москве")
      meta(content="width=device-width, initial-scale=1" name="viewport")
      meta(content="ie=edge" http-equiv="x-ua-compatible")

      link(rel="apple-touch-icon" sizes="180x180" href="favicon/pple-touch-icon.png")
      link(rel="icon" type="image/png" sizes="32x32" href="favicon/favicon-32x32.png")
      link(rel="icon" type="image/png" sizes="16x16" href="favicon/favicon-16x16.png")
      link(rel="manifest" href="favicon/site.webmanifest")
      link(rel="mask-icon" href="favicon/safari-pinned-tab.svg" color="#5bbad5")
      meta(name="msapplication-TileColor" content="#da532c")
      meta(name="theme-color" content="#ffffff")

      link(rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css")
      script(src="https://unpkg.com/swiper@7/swiper-bundle.min.js")

      link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/noUiSlider/15.5.0/nouislider.css")

      link(rel="stylesheet" href="css/style.min.css")

      block title
        title СоюзПрофЭксперт
  body
    include ../includes/common/mixins
    include header
    include footer
    block header
      +header
    block content
    block footer
      +footer
    block scripts
      script(src='scripts/main.min.js')
